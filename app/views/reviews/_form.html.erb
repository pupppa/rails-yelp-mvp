

<div class="col-sm-8 col-sm-offset-2">
<%= form_for(@review) do |f| %>
<div class="form-group">

  <%= f.label :rating %>
  <%= f.text_field :rating, class: "form-control" %>
</div>

<div class="form-group">
  <%= f.label :content %>
  <%= f.text_area :content, class: "form-control", rows: "20" %>
</div>
  <%= f.submit "Update", class: "btn btn-primary" %>


<% end %>
</div>
</div>
